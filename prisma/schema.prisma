generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model LikeImg {
  id     Int    @id @default(autoincrement())
  name   String
  img    String       
  type   String       
  s_desc   String     
  f_desc String @db.Text 
}